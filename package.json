{
  "name": "responsive-landing-page",
  "version": "1.0.0",
  "description": "Responsive Halloween website using HTML, CSS and JavaScript",
  "homepage": "https://github.com/InoreNeronI/responsive-landing-page",
  "scripts": {
    "start": "parcel serve assets/html/*.html --open",
    "build": "parcel build assets/html/*.html --public-url /responsive-landing-page",
    "prestart": "eslint --fix . && prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InoreNeronI/responsive-landing-page.git"
  },
  "keywords": [
    "responsive-website",
    "responsive-landing-page",
    "halloween-website"
  ],
  "author": "Martin Mozos <martinmozos@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InoreNeronI/responsive-landing-page/issues"
  },
  "dependencies": {
    "scrollreveal": "^4.0.9",
    "swiper": "^11.0.5"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.10.3",
    "eslint": "^8.54.0",
    "parcel": "^2.10.3",
    "parcel-reporter-clean-dist": "^1.0.4",
    "posthtml-extend": "^0.6.5",
    "posthtml-include": "^1.7.4",
    "prettier": "3.1.0"
  },
  "alias": {
    "scrollreveal": {
      "global": "ScrollReveal"
    },
    "swiper": {
      "global": "Swiper"
    }
  },
  "targets": {
    "default": {
      "context": "browser"
    }
  }
}
